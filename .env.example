# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# HasData API
NEXT_PUBLIC_HASDATA_API_KEY=your_hasdata_api_key_here

# SMTP Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=analytics@comcreate.org
SMTP_PASS=your_smtp_password_here
SMTP_FROM=analytics@comcreate.org
CONTACT_EMAIL=diego@comcreate.org

# E2E Testing (Playwright)
# Service role key for test data factories (bypasses RLS)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Test user credentials (create a test user in Supabase Auth)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=your_test_user_password

# Base URL for tests (defaults to http://localhost:3000)
BASE_URL=http://localhost:3000
